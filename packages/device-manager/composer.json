{
	"name": "aloisjasa/device-manager",
	"description": "Device Manager",
	"authors": [
		{
			"name": "Alois Ja≈°a",
			"email": "aloisjasa@gmail.com"
		}
	],
	"prefer-stable": true,
	"require": {
		"php": "^8.3",
		"ramsey/uuid": "^4.9"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-doctrine": "^2.0",
		"phpstan/phpstan-nette": "^2.0",
		"phpstan/phpstan-strict-rules": "^2.0",
		"phpunit/phpunit": "^10.5",
		"shipmonk/phpstan-rules": "^4.1",
		"slevomat/coding-standard": "^8.15"
	},
	"autoload": {
		"psr-4": {
			"AloisJasa\\DeviceManager\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AloisJasa\\DeviceManager\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"sort-packages": true
	}
}
